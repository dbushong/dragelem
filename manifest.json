{ "name": "Drag Element"
, "version": "1.0"
, "manifest_version": 2
, "description": "Allows you to drag any element on a webpage around"
, "permissions": [ "contextMenus" ]
, "background": { "scripts": [ "background.js" ] }
, "content_scripts": [ { "matches": [ "<all_urls>" ]
                       , "js": [ "contentscript.js" ]
                       }
                     ]
}
